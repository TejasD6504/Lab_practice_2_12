    <script>
        // Mobile menu toggle
        document.getElementById('menuToggle').addEventListener('click', function() {
            document.getElementById('opt').classList.toggle('active');
        });

        // Trainer search
        const searchInput = document.getElementById("trainerSearch");
        const trainers = document.querySelectorAll("#subpage3 .desc");

        searchInput.addEventListener("keyup", () => {
            let filter = searchInput.value.toLowerCase();
            trainers.forEach(trainer => {
                let text = (trainer.dataset.name + " " + trainer.dataset.specialty).toLowerCase();
                trainer.style.display = text.includes(filter) ? "flex" : "none";
            });
        });

        // Smooth scroll for navigation links
        document.querySelectorAll('#nav a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth' });
                    document.getElementById('opt').classList.remove('active');
                }
            });
        });
    </script>
